{
  "name": "api-engine",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "env-cmd -f ./config/dev.env cross-env NODE_ENV=development ts-node-dev --exit-child --respawn src/index.ts",
    "migrate": "env-cmd -f ./config/dev.env npx prisma migrate dev",
    "migrate:test": "env-cmd -f ./config/test.env npx prisma migrate dev",
    "generate": "npx prisma generate",
    "seed": "env-cmd -f ./config/dev.env prisma db seed",
    "codegen": "graphql-codegen --config codegen.yml",
    "lint": "eslint . --ext .ts --ext .mts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier": "prettier --config .prettierrc --write .",
    "postinstall": "npm run codegen && npm run generate",
    "test": "env-cmd -f ./config/test.env jest --watchAll --detectOpenHandles --runInBand --testTimeout=12000",
    "test:e2e": "env-cmd -f ./config/test.env jest e2e --detectOpenHandles --runInBand --testTimeout=12000"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@graphql-tools/graphql-file-loader": "^8.0.1",
    "@graphql-tools/load": "^8.0.2",
    "@paljs/plugins": "^6.0.7",
    "@prisma/client": "^5.14.0",
    "@whatwg-node/fetch": "^0.9.17",
    "bcryptjs": "^2.4.3",
    "express": "^4.19.2",
    "graphql": "^16.8.1",
    "graphql-yoga": "^5.3.1",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@apollo/client": "^3.10.4",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-typescript": "^7.24.1",
    "@faker-js/faker": "^8.4.1",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.7",
    "@graphql-codegen/typescript-resolvers": "^4.1.0",
    "@graphql-tools/mock": "^9.0.2",
    "@graphql-yoga/apollo-link": "^3.3.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.12",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "babel-jest": "^29.7.0",
    "casual": "^1.6.2",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "eslint": "^9.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-config": "^29.7.0",
    "jest-mock-extended": "^3.0.7",
    "prettier": "^3.2.5",
    "prisma": "^5.14.0",
    "react": "^18.3.1",
    "rimraf": "^5.0.7",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  }
